{
    "compilerOptions": {
        "target": "ES2015",
        "module": "commonjs",
        "outDir": "./dist",
        "lib": [
            "ES2015"
        ],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "types": [
            "k6"
        ]
    },
    "include": [
        "tests/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}